PKG_NAME=libXmu
PKG_VERSION=1.1.0
PKG_URL="$XORG_SRCS/lib/libXmu-$PKG_VERSION.tar.bz2"
PKG_REV=1
PKG_DEPENDS="libX11 libXext libXt"
PKG_RUN_DEPENDS="$TARGET_LIBC"
PKG_BUILD_DEPENDS="toolchain"
PKG_PRIORITY=optional
PKG_SECTION=x11
PKG_SHORTDESC="X11 miscellaneous utility library"
PKG_LONGDESC="libXmu provides a set of miscellaneous utility convenience functions for X libraries to use."
