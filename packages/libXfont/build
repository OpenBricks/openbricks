#!/bin/sh

. config/options

cd $BUILD/$1*

mkdir -p objdir-host
cd objdir-host
do_configure host \
            --disable-ipv6 \
            --disable-devel-docs \

make
make_install toolchain
cd ..

mkdir -p objdir-target
cd objdir-target
do_configure target \
            --disable-ipv6 \
            --disable-devel-docs \

make
make_install
