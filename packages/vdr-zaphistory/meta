PKG_NAME=vdr-zaphistory
PKG_VERSION=0.9.5
PKG_URL="$DISTRO_SRCS/vdr-zaphistory-${PKG_VERSION}.tar.bz2"
PKG_REV=1
PKG_BUILD_DEPENDS="toolchain"
PKG_RUN_DEPENDS="$TARGET_LIBC vdr"
PKG_PRIORITY=optional
PKG_SECTION=multimedia
PKG_SHORTDESC="VDR -- zaphistory plugin"
PKG_LONGDESC="vdr-zaphistory is a plugin for VDR, the Video Disk Recorder. Displays a list of watched channels and statistics."
