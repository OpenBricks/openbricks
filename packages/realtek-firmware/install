#!/bin/sh

. config/options

mkdir -p $INSTALL/firmware/rtl_nic
cp -P $BUILD/linux-firmware-r*/rtl_nic/* $INSTALL/firmware/rtl_nic

mkdir -p $INSTALL/firmware/rtlwifi
cp -P $BUILD/linux-firmware-r*/rtlwifi/* $INSTALL/firmware/rtlwifi

mkdir -p $INSTALL/firmware/RTL8192E
cp -P $BUILD/linux-firmware-r*/RTL8192E/* $INSTALL/firmware/RTL8192E
