PKG_BUILD_DEPENDS="toolchain cmake"
PKG_PRIORITY=optional
PKG_SECTION=misc
PKG_SHORTDESC="iCalendar protocols and data formats"
PKG_LONGDESC="The libical package contains an implementation of the iCalendar protocols and data formats"
