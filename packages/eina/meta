PKG_NAME=eina
PKG_VERSION=1.0.1
PKG_URL="http://download.enlightenment.org/releases/eina-${PKG_VERSION}.tar.bz2"
PKG_REV=1
PKG_BUILD_DEPENDS="toolchain"
PKG_RUN_DEPENDS="$TARGET_LIBC"
PKG_PRIORITY=optional
PKG_SECTION=libs
PKG_SHORTDESC="Enlightenment Foundation Library providing optimized data types"
PKG_LONGDESC="Eina is a multi-platform library that provides optimized data types and a few tools."
