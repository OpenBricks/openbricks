PKG_NAME=kissfft
PKG_VERSION=129
PKG_URL="$SFNET_SRCS/kissfft/kiss_fft${PKG_VERSION}.tar.gz"
PKG_REV=1
PKG_DEPENDS="$TARGET_LIBC"
PKG_BUILD_DEPENDS="toolchain"
PKG_PRIORITY=optional
PKG_SECTION=libs
PKG_SHORTDESC="simple FFT library"
PKG_LONGDESC="A Fast Fourier Transform based up on the principle, 'Keep It Simple, Stupid.' Kiss FFT is a very small, reasonably efficient, mixed radix FFT library that can use either fixed or floating point data types."
