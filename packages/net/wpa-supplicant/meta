PKG_NAME=wpa-supplicant
PKG_VERSION=2.9
PKG_URL="https://w1.fi/releases/wpa_supplicant-${PKG_VERSION}.tar.gz"
PKG_REV="1"
PKG_RUN_DEPENDS="$TARGET_LIBC"
PKG_BUILD_DEPENDS="toolchain"
PKG_DEPENDS="dbus libnl libgcrypt libgpg-error openssl"
PKG_PRIORITY=optional
PKG_SECTION=net
PKG_SHORTDESC="client support for WPA and WPA2 (IEEE 802.11i)"
PKG_LONGDESC="WPA and WPA2 are methods for securing wireless networks, the former using IEEE 802.1X, and the latter using IEEE 802.11i. This software provides key negotiation with the WPA Authenticator, and controls association with IEEE 802.11i networks."
