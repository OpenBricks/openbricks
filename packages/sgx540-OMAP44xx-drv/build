#!/bin/sh

. config/options

cd $BUILD/$1*/sgx

cd eurasiacon/build/linux/omap4430_linux/kbuild
make KERNELDIR=$(kernel_path) CROSS_COMPILE=${TARGET_PREFIX}
cd -

MOD_DIR=".install/lib/modules/$(kernel_version)/kernel/extra"

mkdir -p ${MOD_DIR}
for m in pvrsrvkm omaplfb bc_example; do
  cp -P eurasiacon/binary_omap4430_linux_release/${m}.ko $MOD_DIR
done
