#!/bin/sh

. config/options
get_meta $1

cd $PKG_BUILD_DIR

cd objdir-host
do_installdev usr/bin/ecppc toolchain
do_installdev usr/include toolchain
do_installdev usr/lib toolchain

cd ..

cd objdir-target
do_installdev usr/bin/tntnet-config toolchain
do_installdev usr/include
do_installdev usr/lib
