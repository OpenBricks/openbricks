PKG_NAME=acct
PKG_VERSION=6.5.5
PKG_URL="http://ftp.gnu.org/gnu/acct/acct-${PKG_VERSION}.tar.gz"
PKG_REV=1
PKG_BUILD_DEPENDS="toolchain"
PKG_RUN_DEPENDS="$TARGET_LIBC"
PKG_PRIORITY=optional
PKG_SECTION=admin
PKG_SHORTDESC="The GNU Accounting utilities for process and login accounting"
PKG_LONGDESC="GNU Accounting Utilities is a set of utilities which reports and summarizes data about user connect times and process execution statistics."
