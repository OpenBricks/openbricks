PKG_BUILD_DEPENDS="toolchain gstreamer"
PKG_PRIORITY=optional
PKG_SECTION=multimedia
PKG_SHORTDESC="OMAP3 DSP plugin for GStreamer"
PKG_LONGDESC="This GStreamer plugin provides video acceleration through the OMAP3 DSP."
