PKG_NAME=flatbuffers
PKG_VERSION=1.12.0
PKG_URL="https://github.com/google/flatbuffers/archive/v${PKG_VERSION}.tar.gz"
PKG_ARCHIVE_NAME="${PKG_NAME}-${PKG_VERSION}.tar.gz"
PKG_REV=1
PKG_DEPENDS="$TARGET_LIBC"
PKG_BUILD_DEPENDS="toolchain"
PKG_PRIORITY=optional
PKG_SECTION=libs
PKG_SHORTDESC="FlatBuffers"
PKG_LONGDESC="FlatBuffers is a cross platform serialization library architected for maximum memory efficiency."
