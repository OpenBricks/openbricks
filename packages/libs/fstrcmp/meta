PKG_NAME=fstrcmp
PKG_VERSION=0.7
PKG_URL="http://fstrcmp.sourceforge.net/fstrcmp-${PKG_VERSION}.D001.tar.gz"
PKG_REV=1
PKG_DEPENDS="$TARGET_LIBC"
PKG_BUILD_DEPENDS="toolchain"
PKG_PRIORITY=optional
PKG_SECTION=libs
PKG_SHORTDESC="fstrcmp"
PKG_LONGDESC="A library that is used to make fuzzy comparisons of strings and byte arrays"
