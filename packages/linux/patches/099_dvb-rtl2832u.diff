+	TUA9001_BANDWIDTH_6MHZ = TUNER_BANDWIDTH_6MHZ,
+	TUA9001_BANDWIDTH_7MHZ = TUNER_BANDWIDTH_7MHZ,
+	TUA9001_BANDWIDTH_8MHZ = TUNER_BANDWIDTH_8MHZ,
+};
