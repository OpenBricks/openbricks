diff -Naur linux-2.6-oldvm/arch/i386/boot/compressed/LzmaDecode.c linux-2.6-newvm/arch/i386/boot/compressed/LzmaDecode.c
--- linux-2.6-oldvm/arch/i386/boot/compressed/LzmaDecode.c	1969-12-31 19:00:00.000000000 -0500
+++ linux-2.6-newvm/arch/i386/boot/compressed/LzmaDecode.c	2005-06-30 03:09:20.270000000 -0400
@@ -0,0 +1,588 @@
+/*
+  LzmaDecode.c
+  LZMA Decoder (optimized for Speed version)
+  
+  LZMA SDK 4.22 Copyright (c) 1999-2005 Igor Pavlov (2005-06-10)
+  http://www.7-zip.org/
+
