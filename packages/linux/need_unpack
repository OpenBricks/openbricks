#!/bin/sh

. config/options

STAMP=$STAMPS/$1/unpack

test "$CONFIG/platforms/$TARGET_PLATFORM/machines/$TARGET_MACHINE/linux.conf" -nt $STAMP && rm -f $STAMP
test "$CONFIG/platforms/$TARGET_PLATFORM/linux.conf" -nt $STAMP && rm -f $STAMP
test $PACKAGES/$1/config/$1.$TARGET_ARCH.conf -nt $STAMP && rm -f $STAMP
test $PACKAGES/$1/config/$1.$TARGET_PLATFORM.conf -nt $STAMP && rm -f $STAMP

exit 0
