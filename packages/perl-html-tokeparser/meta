PKG_NAME=perl-html-tokeparser
PKG_VERSION=3.68
PKG_URL="$CPAN_SRCS/G/GA/GAAS/HTML-Parser-${PKG_VERSION}.tar.gz"
PKG_REV=1
PKG_RUN_DEPENDS="$TARGET_LIBC"
PKG_BUILD_DEPENDS="toolchain"
PKG_DEPENDS="perl perl-html-tagset"
PKG_PRIORITY=optional
PKG_SECTION=perl
PKG_SHORTDESC="HTML::TokeParser - Alternative HTML::Parser interface"
PKG_LONGDESC="The HTML::TokeParser is an alternative interface to the HTML::Parser class. It is an HTML::PullParser subclass with a predeclared set of token types. If you wish the tokens to be reported differently you probably want to use the HTML::PullParser directly."
