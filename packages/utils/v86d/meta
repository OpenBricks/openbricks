PKG_NAME=v86d
PKG_VERSION=0.1.10
PKG_URL="$SFNET_SRCS/openpctv/sources/${PKG_NAME}-${PKG_VERSION}.tar.bz2"
PKG_REV=1
PKG_BUILD_DEPENDS="toolchain"
PKG_PRIORITY=optional
PKG_SECTION=utils
PKG_SHORTDESC="A x86 Emulation Daemon"
PKG_LONGDESC="v86d provides a backend for kernel drivers that need to execute x86 BIOS code.  The code is executed in a controlled environment and the results are passed back to the kernel via the netlink interface."
