PKG_NAME=musicbrainzngs
PKG_VERSION=0.5
PKG_URL="https://pypi.python.org/packages/source/m/$PKG_NAME/$PKG_NAME-$PKG_VERSION.tar.gz"
PKG_REV=1
PKG_DEPENDS="$TARGET_LIBC Python libmusicbrainz"
PKG_BUILD_DEPENDS="toolchain python-distribute"
PKG_PRIORITY=optional
PKG_SECTION=python
PKG_SHORTDESC="python bindings for musicbrainz NGS webservice"
PKG_LONGDESC="This library implements webservice bindings for the Musicbrainz NGS site, also known as /ws/2."
