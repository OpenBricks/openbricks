PKG_NAME=libXdmcp
PKG_VERSION=1.1.1
PKG_URL="$XORG_SRCS/lib/libXdmcp-${PKG_VERSION}.tar.bz2"
PKG_REV=1
PKG_DEPENDS="$TARGET_LIBC"
PKG_BUILD_DEPENDS="toolchain xproto"
PKG_PRIORITY=optional
PKG_SECTION=x11
PKG_SHORTDESC="X11 Display Manager Control Protocol library"
PKG_LONGDESC="This package provides the main interface to the X11 display manager control protocol library, which allows for remote logins to display managers"
