PKG_NAME=openssl
PKG_VERSION=1.0.0e
PKG_URL="http://openssl.org/source/${PKG_NAME}-${PKG_VERSION}.tar.gz"
PKG_REV=1
PKG_DEPENDS="zlib cryptodev"
PKG_RUN_DEPENDS="$TARGET_LIBC"
PKG_BUILD_DEPENDS="toolchain"
PKG_PRIORITY=optional
PKG_SECTION=libs
PKG_SHORTDESC="Secure Socket Layer (SSL) library"
PKG_LONGDESC="This is the OpenSSL implementation of Secure Socket Layer (SSL)."
