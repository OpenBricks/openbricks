PKG_VERSION=1.7.3
PKG_REV=1
PKG_BUILD_DEPENDS="toolchain"
PKG_PRIORITY=optional
PKG_SECTION=multimedia
PKG_SHORTDESC="Multi Multicast DVB"
PKG_LONGDESC="MuMuDVB is a program that redistributes streams from DVB (Digital Television) on a network (also called IPTV) using multicasting or HTTP unicast. It can multicast a whole DVB transponder by assigning each channel a different multicast IP. It detects the different services present, rewrite the main DVB tables to show clients only the right stream in each group."
