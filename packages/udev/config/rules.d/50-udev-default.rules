# do not edit this file, it will be overwritten on update

SUBSYSTEM=="block", SYMLINK+="block/%M:%m"
SUBSYSTEM!="block", SYMLINK+="char/%M:%m"

KERNEL=="pty[pqrstuvwxyzabcdef][0123456789abcdef]"
KERNEL=="tty[pqrstuvwxyzabcdef][0123456789abcdef]"
KERNEL=="ptmx"
KERNEL=="tty"
KERNEL=="tty[0-9]*"
KERNEL=="s3c2410_serial[0-9]",  NAME="ttySAC%n"
KERNEL=="console"
KERNEL=="vcs|vcs[0-9]*|vcsa|vcsa[0-9]*"

# serial
KERNEL=="tty[A-Z]*[0-9]|pppox[0-9]*|ircomm[0-9]*|noz[0-9]*|rfcomm[0-9]*"
KERNEL=="ppp"
KERNEL=="mwave",		NAME="modems/mwave"
KERNEL=="hvc*|hvsi*"

# mem
KERNEL=="null|zero|full|random|urandom"
KERNEL=="mem|kmem|port|nvram"

# input
KERNEL=="mouse*|mice|event*",	NAME="input/%k"
KERNEL=="ts[0-9]*|uinput",	NAME="input/%k"
KERNEL=="js[0-9]*",		NAME="input/%k"

# video4linux
SUBSYSTEM=="video4linux"
KERNEL=="vttuner*"
KERNEL=="vtx*|vbi*"
KERNEL=="winradio*"

# graphics
KERNEL=="agpgart"
KERNEL=="card[0-9]*",		NAME="dri/%k"
KERNEL=="pmu"
KERNEL=="nvidia*|nvidiactl*"
SUBSYSTEM=="graphics"
SUBSYSTEM=="drm"

# DVB (video)
SUBSYSTEM=="dvb", ENV{DVB_ADAPTER_NUM}=="?*", NAME="dvb/adapter$env{DVB_ADAPTER_NUM}/$env{DVB_DEVICE_TYPE}$env{DVB_DEVICE_NUM}"

# FireWire (dv1394 and video1394 drivers)
KERNEL=="dv1394-[0-9]*",	NAME="dv1394/%n"
KERNEL=="video1394-[0-9]*",	NAME="video1394/%n"
# FireWire (firewire-core driver: IIDC devices, AV/C devices)
SUBSYSTEM=="firewire", ATTR{units}=="*0x00a02d:0x00010*"
SUBSYSTEM=="firewire", ATTR{units}=="*0x00a02d:0x010001*"

# libusb device nodes
SUBSYSTEM=="usb", ENV{DEVTYPE}=="usb_device", NAME="bus/usb/$env{BUSNUM}/$env{DEVNUM}"

# printer
KERNEL=="parport[0-9]*"
SUBSYSTEM=="printer",		KERNEL=="lp*"
SUBSYSTEM=="ppdev"
SUBSYSTEM=="usb",		KERNEL=="lp*", NAME="usb/%k", SYMLINK+="usb%k"
KERNEL=="lp[0-9]*"
KERNEL=="irlpt[0-9]*"

# block
SUBSYSTEM=="block"

# floppy
KERNEL=="fd[0-9]"
KERNEL=="fd[0-9]", ACTION=="add", ATTRS{cmos}=="?*", RUN+="create_floppy_devices -c -t $attr{cmos} -m %M -M 0640 -G floppy $root/%k"
KERNEL=="hd*", SUBSYSTEMS=="ide", ATTRS{media}=="floppy", OPTIONS+="all_partitions"

# cdrom
SUBSYSTEM=="block", KERNEL=="sr[0-9]*", SYMLINK+="scd%n"
SUBSYSTEM=="block", KERNEL=="hd*", SUBSYSTEMS=="ide", ATTRS{media}=="cdrom"
SUBSYSTEM=="scsi_generic", SUBSYSTEMS=="scsi", ATTRS{type}=="4|5"
KERNEL=="pktcdvd[0-9]*", NAME="pktcdvd/%k"
KERNEL=="pktcdvd", NAME="pktcdvd/control"

# tape
KERNEL=="ht[0-9]*|nht[0-9]*"
KERNEL=="pt[0-9]*|npt[0-9]*|pht[0-9]*"
SUBSYSTEM=="scsi_generic|scsi_tape", SUBSYSTEMS=="scsi", ATTRS{type}=="1|8"

# block-related
KERNEL=="sch[0-9]*"
SUBSYSTEM=="scsi_generic", SUBSYSTEMS=="scsi", ATTRS{type}=="0"
KERNEL=="pg[0-9]*"
KERNEL=="qft[0-9]*|nqft[0-9]*|zqft[0-9]*|nzqft[0-9]*|rawqft[0-9]*|nrawqft[0-9]*"
KERNEL=="rawctl", NAME="raw/rawctl"
SUBSYSTEM=="raw", KERNEL=="raw[0-9]*", NAME="raw/%k"
SUBSYSTEM=="bsg", NAME="bsg/%k"
SUBSYSTEM=="aoe", NAME="etherd/%k"
SUBSYSTEM=="aoe", KERNEL=="err"

# network
KERNEL=="tun",			NAME="net/%k"
KERNEL=="rfkill"

# CPU
KERNEL=="cpu[0-9]*",		NAME="cpu/%n/cpuid"
KERNEL=="msr[0-9]*",		NAME="cpu/%n/msr"
KERNEL=="microcode",		NAME="cpu/microcode"

# miscellaneous
KERNEL=="fuse"
SUBSYSTEM=="rtc", DRIVERS=="rtc_cmos", SYMLINK+="rtc"
KERNEL=="auer[0-9]*",		NAME="usb/%k"
KERNEL=="hw_random",		NAME="hwrng"
KERNEL=="mmtimer"
KERNEL=="rflash[0-9]*"
KERNEL=="rrom[0-9]*"
KERNEL=="sxctl",		NAME="specialix_sxctl"
KERNEL=="rioctl",		NAME="specialix_rioctl"
KERNEL=="iowarrior[0-9]*",	NAME="usb/%k"
KERNEL=="hiddev[0-9]*",		NAME="usb/%k"
KERNEL=="legousbtower[0-9]*",	NAME="usb/%k"
KERNEL=="dabusb[0-9]*",		NAME="usb/%k"
KERNEL=="usbdpfp[0-9]*",	NAME="usb/%k"
KERNEL=="cpad[0-9]*",		NAME="usb/%k"

# do not delete static device nodes
ACTION=="remove", NAME=="?*", TEST=="/lib/udev/devices/$name", OPTIONS+="ignore_remove"
ACTION=="remove", NAME=="", TEST=="/lib/udev/devices/%k", OPTIONS+="ignore_remove"
