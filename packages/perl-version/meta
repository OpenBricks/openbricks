PKG_NAME=perl-version
PKG_VERSION=0.88
PKG_URL="$CPAN_SRCS/J/JP/JPEACOCK/version-${PKG_VERSION}.tar.gz"
PKG_REV=1
PKG_RUN_DEPENDS="$TARGET_LIBC"
PKG_BUILD_DEPENDS="toolchain"
PKG_DEPENDS=perl
PKG_PRIORITY=optional
PKG_SECTION=perl
PKG_SHORTDESC="Perl extension for Version Objects"
PKG_LONGDESC="This module implements version objects for older version of Perl and provides the version object API for all versions of Perl."
