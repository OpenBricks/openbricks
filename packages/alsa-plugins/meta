PKG_NAME=alsa-plugins
PKG_VERSION=1.0.26
PKG_URL="http://ftp.osuosl.org/pub/blfs/conglomeration/alsa-plugins/${PKG_NAME}-${PKG_VERSION}.tar.bz2"
PKG_REV=1
PKG_RUN_DEPENDS="$TARGET_LIBC alsa-lib pulseaudio speex"
PKG_BUILD_DEPENDS="toolchain alsa-lib pulseaudio"
PKG_PRIORITY=standard
PKG_SECTION=sound
PKG_SHORTDESC="ALSA library additional plugins"
PKG_LONGDESC="This package contains additional plugins for the ALSA library."
