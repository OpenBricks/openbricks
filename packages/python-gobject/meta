PKG_NAME=python-gobject
PKG_VERSION=2.28.0
PKG_URL="http://ftp.gnome.org/pub/GNOME/sources/pygobject/2.28/pygobject-${PKG_VERSION}.tar.bz2"
PKG_REV=1
PKG_DEPENDS="GLib Python"
PKG_RUN_DEPENDS="$TARGET_LIBC"
PKG_BUILD_DEPENDS="toolchain"
PKG_PRIORITY=optional
PKG_SECTION=python
PKG_SHORTDESC="Python bindings for the GObject library"
PKG_LONGDESC="GObject is an abstraction layer that allows programming with an object paradigm that is compatible with many languages. It is a part of Glib, the core library used to build GTK+ and GNOME. This package contains the Python bindings for GObject. It is mostly used by other bindings to map their GObjects to Python objects."
