PKG_NAME=perl-list-moreutils
PKG_VERSION=0.32
PKG_URL="$CPAN_SRCS/A/AD/ADAMK/List-MoreUtils-${PKG_VERSION}.tar.gz"
PKG_REV=1
PKG_RUN_DEPENDS="$TARGET_LIBC"
PKG_BUILD_DEPENDS="toolchain"
PKG_DEPENDS=perl
PKG_PRIORITY=optional
PKG_SECTION=perl
PKG_SHORTDESC="Provide the stuff missing in List::Util"
PKG_LONGDESC="List::MoreUtils provides some trivial but commonly needed functionality on lists which is not going to go into List::Util."
