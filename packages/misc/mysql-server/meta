PKG_PARENT=mysql
PKG_NAME=${PKG_PARENT}-server
PKG_RUN_DEPENDS="$TARGET_LIBC mysql-client libaio"
PKG_PRIORITY=optional
PKG_SECTION=misc
PKG_SHORTDESC="MySQL database server"
PKG_LONGDESC="MySQL is a SQL (Structured Query Language) database server. SQL is the most popular database language in the world. MySQL is a client server implementation that consists of a server daemon mysqld and many different client programs/libraries. This package contains the MySQL server daemon and its supporting files."
PKG_REV=2
